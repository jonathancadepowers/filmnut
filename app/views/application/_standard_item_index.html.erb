<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Created At</th>
      <th scope="col">Updated At</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
    <% object_collection(controller_name).each do |o| %>
      <tr>
        <td>
          <%= o.title %>
        </td>
        <td>
          <%= o.created_at.strftime("%b %e, %Y") %>
        </td>
        <td>
          <%= o.updated_at.strftime("%b %e, %Y") %>
        </td>
        <td class="blank_links"><a href ="<%= object_edit_path(o) %>"><%= fa_icon "edit" %></a></td>
      </tr>
    <% end %>
  </tbody>
</table>
